function [SL] = SL(rr, F(k), Nr)

    c0 = 3e8;
    
    
    Str = load('Xmn.mat');
    Xmn = Str.Xmn;
    
    beta_rho = zeros(length(Nr), length(Nr));
    beta = zeros(length(Nr), length(Nr));
    
    for i = 1:length(Nr)
        beta(i, i) = (2 * pi * F) ./ c0;
        beta_rho(i, i) = (Xmn(i).xmn)./rr;
    end
    
    bet
end